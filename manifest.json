{
  "name": "Innocent",
  "description": "Automatically clean up the browsing history of certain websites",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "storage",
    "history",
    "cookies",
    "tabs"
  ]
}